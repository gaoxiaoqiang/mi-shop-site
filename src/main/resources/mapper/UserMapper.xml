<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tst.mall.mapper.UserMapper">

    <select id="findByUserName"
            resultType="com.tst.mall.model.entity.User">
        select * from tb_mall_user where 1=1 and  user_name = #{userName}
    </select>





</mapper>